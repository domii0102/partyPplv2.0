model EventGuest {
  guestId          Int      @id @default(autoincrement())
  userId           String
  eventId          Int
  confirmedArrival Boolean

  userCredentials UserCredentials @relation(fields: [userId], references: [userId])
  event           Event           @relation(fields: [eventId], references: [eventId])

  @@unique([userId, eventId]) 
  @@index([userId])
  @@index([eventId])

  @@map("EventGuest")
}